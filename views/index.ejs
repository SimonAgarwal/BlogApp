<%- include ("partials/header")%>
<div class="ui container">
<h1> 
	Index page
</h1>
<% blogs.forEach(function(blog){%>
						   <div>
							   <h2>
								   <a href="/blogs/<%=blog._id%>"> <%= blog.title %></a>
							   </h2>
							 <img src="<%=blog.image%>">
							   <span><%= blog.created%></span>
								   <p><%=blog.body.substring(0,100)%>....</p>
							   </div>
						   
						   <%})%>
</div>
<%- include ("partials/footer")%>